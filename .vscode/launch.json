    "version": "0.2.0",
    "configurations": [
        {
            "name": "Node - Labels",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/nodejs/index.js",
            "args": [
                "${workspaceFolder}/data/labels-20180514_test56.xml"
            ],
        },
        {
            "name": "Python - Labels",
            "type": "pythonExperimental",
            "request": "launch",
            "program": "${workspaceFolder}/python/main.py",
            "args": [
                "${workspaceFolder}/data/labels-20180514_test56.xml"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/dotnet-core/bin/Debug/netcoreapp2.0/xmlperformance.dll",
            "args": [
                "${workspaceFolder}/data/artists-20180514_test50.xml"
            ],
            "cwd": "${workspaceFolder}/dotnet-core",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}